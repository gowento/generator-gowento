{
  "name": "generator-gowento",
  "version": "1.4.0",
  "description": "Node module template for authoring with ES6+, Babel, AVA and npm scripts",
  "license": "MIT",
  "main": "app/index.js",
  "scripts": {
    "deploy": "git pull --rebase origin master && git push origin master",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "postpublish": "git push origin master --follow-tags"
  },
  "author": "Gowento",
  "contributors": [
    "Loris Guignard <loris@gowento.com>",
    "Eric Redon <eric@gowento.com>"
  ],
  "keywords": [
    "yeoman-generator"
  ],
  "dependencies": {
    "camelcase": "4.1.0",
    "slug": "0.9.1",
    "yeoman-generator": "1.1.1",
    "yosay": "2.0.0"
  },
  "devDependencies": {
    "eslint": "3.19.0",
    "eslint-config-gowento": "4.0.2"
  }
}
