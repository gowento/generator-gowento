{
  "name": "generator-gowento",
  "version": "1.4.0",
  "description": "Node module template for authoring with ES6+, Babel, AVA and npm scripts",
  "license": "MIT",
  "main": "app/index.js",
  "scripts": {
    "deploy": "git pull --rebase origin master && git push origin master",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "postpublish": "git push origin master --follow-tags"
  },
  "author": "Gowento",
  "contributors": [
    "Loris Guignard <loris@gowento.com>",
    "Eric Redon <eric@gowento.com>"
  ],
  "keywords": [
    "yeoman-generator"
  ],
  "dependencies": {
    "camelcase": "^3.0.0",
    "generator-babel": "2.0.2",
    "generator-git-init": "1.1.3",
    "generator-travis": "^1.3.0",
    "slug": "0.9.1",
    "yeoman-generator": "^0.23.3",
    "yosay": "1.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-preset-es2015": "6.6.0"
  }
}
