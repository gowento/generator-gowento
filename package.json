{
  "name": "generator-gowento",
  "version": "2.0.0",
  "description": "Node module template for authoring with ES6+, Babel, AVA and npm/yarn scripts",
  "license": "MIT",
  "main": "app/index.js",
  "scripts": {
    "deploy": "git pull --rebase origin master && git push origin master",
    "patch": "yarn version patch && yarn publish",
    "minor": "yarn version minor && yarn publish",
    "major": "yarn version major && yarn publish",
    "postpublish": "git push origin master --follow-tags"
  },
  "author": "Gowento",
  "contributors": [
    "Loris Guignard <loris@gowento.com>",
    "Eric Redon <eric@gowento.com>"
  ],
  "keywords": [
    "yeoman-generator"
  ],
  "dependencies": {
    "camelcase": "4.1.0",
    "slug": "0.9.1",
    "yeoman-generator": "1.1.1",
    "yosay": "2.0.0"
  },
  "devDependencies": {
    "eslint": "3.19.0",
    "eslint-config-gowento": "4.0.2"
  }
}
