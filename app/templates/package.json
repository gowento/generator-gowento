{
  "name": "<%= moduleName %>",
  "version": "1.0.0",
  "description": "<%= moduleDesc %>",
  "author": "Gowento <hackers@gowento.com>",
  "contributors": [
    "<%= name %> <<%= email %>>"
  ],
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src test --cache",
    "prebuild": "yarn run clean -s",
    "build": "babel --optional runtime src -d dist",
    "postbuild": "yarn run test -s",
    "test": "ava",
    "prepublish": "yarn run build -s",
    "deploy": "git pull --rebase origin master && git push origin master",
    "patch": "yarn version patch && yarn publish",
    "minor": "yarn version minor && yarn publish",
    "major": "yarn version major && yarn publish",
    "postpublish": "git push origin master --follow-tags"
  },
  "ava": {
    "failFast": false,
    "verbose": false,
    "require": [
      "babel-core/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gowento/<%= moduleName %>.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/gowento/<%= moduleName %>#readme",
  "devDependencies": {
    "ava": "*",
    "babel-cli": "*",
    "eslint": "^3",
    "eslint-config-gowento": "*",
    "rimraf": "*",
    "babel-plugin-transform-object-rest-spread": "*",
    "babel-preset-env": "*"
  }
}
